@model List<string>

@{
    ViewData["Title"] = $"{ViewData["ActionName"]} Results";
}

<h1>@ViewData["ActionName"] Results</h1>

@if (Model != null && Model.Any())
{
    <ul>
        @foreach (var result in Model)
        {
            <li>@result</li>
        }
    </ul>
}
else
{
    <p>No results to display.</p>
}

<a asp-action="Index" class="btn btn-primary">Back to Enclosures</a>
